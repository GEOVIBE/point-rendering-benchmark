{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": false,
	"arrowParens": "always",
	"semi": true,
	"tabWidth": 2,
	"printWidth": 80,
	"htmlWhitespaceSensitivity": "css",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
